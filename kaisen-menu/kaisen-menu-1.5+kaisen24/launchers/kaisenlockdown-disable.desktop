#!/usr/bin/env xdg-open

[Desktop Entry]
Name=lockdown-disable
Name[fr]=lockdown-désactivé
Name[es]=lockdown-desactivado
Name[pt]=lockdown-desativado
Name[it]=lockdown-disabilitato
Name[de]=lockdown-deaktiviert
Comment=Disable lockdown service
Comment[fr]=Désactiver le service lockdown
Comment[es]=Deshabilitar servicio lockdown
Comment[pt]=Desativar serviço lockdown
Comment[it]=Disabilita servizio lockdown
Comment[de]=Deaktivieren Sie den lockdown
Exec=kaisen-launcher --command "/opt/services/lockdown-disable" --sudo
Icon=utilities-terminal
Terminal=false
Type=Application
StartupNotify=true
Categories=SLockdown
X-Kaisen-Package=lockdown
